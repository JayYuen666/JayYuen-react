{
  "name": "JayYuen-react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --write src",
    "lint-fix": "eslint --fix src --ext .js,.ts,.tsx",
    "release": "release-it",
    "release:changelog:fix": "node ./bin/injectContributorsList.js && git add CHANGELOG.md",
    "release:dry": "release-it --dry-run --no-npm",
    "release:info": "release-it --release-version",
    "release:beta:no-npm": "release-it --preRelease=beta --no-npm",
    "release:beta": "release-it --preRelease=beta",
    "release:no-npm": "release-it --no-npm"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "dependencies": {
    "@unocss/reset": "^0.51.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.19.1",
    "@iconify-json/svg-spinners": "^1.1.1",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/node": "^20.1.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@unocss/eslint-config": "^0.51.13",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "chalk": "^5.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "handlebars": "^4.7.7",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "lodash-es": "^4.17.21",
    "prettier": "^2.8.8",
    "release-it": "^15.10.3",
    "string-replace-async": "^3.0.2",
    "typescript": "^5.0.2",
    "unocss": "^0.51.13",
    "vite": "^4.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
